                        </term_definitions>
                        <term_definitions code="SNOMED-CT::SCTID::21454007">
                            <items id="text">SAH (Subarachnoid hemorrhage)</items>
                        </term_definitions>
                        <term_definitions code="SNOMED-CT::SCTID::266257000">
                            <items id="text">TIA (Transient ischemic attack)</items>
                        </term_definitions>
                    </children>
