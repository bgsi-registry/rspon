                        <term_definitions code="at0000">
                            <items id="text">NIH Stroke Scale</items>
                            <items id="description">15-item neurologic examination stroke scale that provides a quantitative measure of stroke-related neurological deficit.</items>
                        </term_definitions>
                        <term_definitions code="at0001">
                            <items id="text">*Event Series(pt)</items>
                            <items id="description">*@ internal @(pt)</items>
                        </term_definitions>
                        <term_definitions code="at0002">
                            <items id="text">Baseline</items>
                            <items id="description">Baseline assessment.</items>
                        </term_definitions>
                        <term_definitions code="at0003">
                            <items id="text">*Tree(pt)</items>
                            <items id="description">*@ internal @(pt)</items>
                        </term_definitions>
                        <term_definitions code="at0005">
                            <items id="text">LOC responsiveness</items>
                            <items id="description">Observed level of consciousness of the patient.</items>
                        </term_definitions>
                        <term_definitions code="at0006">
                            <items id="text">Alert</items>
                            <items id="description">Keenly responsive.</items>
                        </term_definitions>
                        <term_definitions code="at0007">
                            <items id="text">Not alert, but arousable by minor stimulation</items>
                            <items id="description">Not alert; but arousable by minor stimulation to obey, answer or respond.</items>
                        </term_definitions>
                        <term_definitions code="at0008">
                            <items id="text">Not alert, requires repeated stimulation or strong stimuli.</items>
                            <items id="description">Not alert; requires repeated stimulation to attend, or is obtunded and requires strong or painful stimulation to make movements.</items>
                        </term_definitions>
                        <term_definitions code="at0009">
                            <items id="text">Reflex reponses only or totally unreponsive</items>
                            <items id="description">Responds only with reflex motor or autonomic effects, or totally unresponsive, flaccid, and areflexic.</items>
                        </term_definitions>
                        <term_definitions code="at0010">
                            <items id="text">LOC questions</items>
                            <items id="description">The patient is asked the month and his/her age.</items>
                        </term_definitions>
                        <term_definitions code="at0011">
                            <items id="text">Both correct</items>
                            <items id="description">Patient answered both questions correctly.</items>
                        </term_definitions>
                        <term_definitions code="at0012">
                            <items id="text">One correct</items>
                            <items id="description">Patient answered one question correctly or is unable to communicate because of a problem not secondary to aphasia.</items>
                        </term_definitions>
                        <term_definitions code="at0013">
                            <items id="text">Neither correct</items>
                            <items id="description">Patient answered neither question correctly or is comatose or does not comprehend the question.</items>
                        </term_definitions>
                        <term_definitions code="at0014">
                            <items id="text">LOC commands</items>
                            <items id="description">The patient is asked to open and close the eyes and then to grip and release the non-paretic hand.</items>
                        </term_definitions>
                        <term_definitions code="at0015">
                            <items id="text">Both correct</items>
                            <items id="description">Patient performs both tasks correctly.</items>
                        </term_definitions>
                        <term_definitions code="at0016">
                            <items id="text">One correct</items>
                            <items id="description">Patient performs one task correctly.</items>
                        </term_definitions>
                        <term_definitions code="at0017">
                            <items id="text">Neither correct</items>
                            <items id="description">Patient performs neither task correctly.</items>
                        </term_definitions>
                        <term_definitions code="at0018">
                            <items id="text">Horizontal eye movements</items>
                            <items id="description">Horizontal eye movement observation.</items>
                        </term_definitions>
                        <term_definitions code="at0019">
                            <items id="text">Normal</items>
                            <items id="description">Normal eye movement.</items>
                        </term_definitions>
                        <term_definitions code="at0020">
                            <items id="text">Partial gaze palsy</items>
                            <items id="description">Gaze is abnormal in one or both eyes, but forced deviation or total gaze paresis is not present.</items>
                        </term_definitions>
                        <term_definitions code="at0021">
                            <items id="text">Forced deviation or total gaze paresis</items>
                            <items id="description">Forced deviation or total gaze paresis is not overcome by the oculocephalic maneuver.</items>
                        </term_definitions>
                        <term_definitions code="at0022">
                            <items id="text">Visual fields</items>
                            <items id="description">Visual field test observation.</items>
                        </term_definitions>
                        <term_definitions code="at0023">
                            <items id="text">No visual loss</items>
                            <items id="description">No visual loss detected.</items>
                        </term_definitions>
                        <term_definitions code="at0024">
                            <items id="text">Partial hemianopia</items>
                            <items id="description">Partial hemianopia or complete quadrantanopia detected.</items>
                        </term_definitions>
                        <term_definitions code="at0025">
                            <items id="text">Complete hemianopia</items>
                            <items id="description">Complete hemianopia detected</items>
                        </term_definitions>
                        <term_definitions code="at0026">
                            <items id="text">Bilateral hemianopia</items>
                            <items id="description">Blindness from any cause, including cortical blindness.</items>
                        </term_definitions>
                        <term_definitions code="at0027">
                            <items id="text">Facial palsy</items>
                            <items id="description">Facial palsy observation.</items>
                        </term_definitions>
                        <term_definitions code="at0028">
                            <items id="text">Normal</items>
                            <items id="description">Normal symmetrical movements.</items>
                        </term_definitions>
                        <term_definitions code="at0029">
                            <items id="text">Minor paralysis</items>
                            <items id="description">Flattened nasolabial fold, asymmetry on smiling.</items>
                        </term_definitions>
                        <term_definitions code="at0030">
                            <items id="text">Partial paralysis</items>
                            <items id="description">Total or near-total paralysis of lower face.</items>
                        </term_definitions>
                        <term_definitions code="at0031">
                            <items id="text">Complete paralysis of one or both sides</items>
                            <items id="description">Absence of facial movement in the upper and lower face.</items>
                        </term_definitions>
                        <term_definitions code="at0075">
                            <items id="text">Sensory</items>
                            <items id="description">Observation of sensory loss.</items>
                        </term_definitions>
                        <term_definitions code="at0076">
                            <items id="text">Normal</items>
                            <items id="description">No sensory loss.</items>
                        </term_definitions>
                        <term_definitions code="at0077">
                            <items id="text">Mild-to-moderate sensory loss</items>
                            <items id="description">Patient feels pinprick is less sharp or is dull on the affected side; or there is a loss of superficial pain with pinprick, but patient is aware of being touched.</items>
                        </term_definitions>
                        <term_definitions code="at0078">
                            <items id="text">Severe or total sensory loss</items>
                            <items id="description">Patient is not aware of being touched in the face, arm, and leg.</items>
                        </term_definitions>
                        <term_definitions code="at0079">
                            <items id="text">Language</items>
                            <items id="description">Observation of language capability.</items>
                        </term_definitions>
                        <term_definitions code="at0080">
                            <items id="text">No aphasia</items>
                            <items id="description">Normal</items>
                        </term_definitions>
                        <term_definitions code="at0081">
                            <items id="text">Mild-to-moderate aphasia</items>
                            <items id="description">Some obvious loss of fluency or facility of comprehension, without significant limitation on ideas expressed or form of expression.</items>
                        </term_definitions>
                        <term_definitions code="at0082">
                            <items id="text">Severe aphasia</items>
                            <items id="description">All communication is through fragmentary expression; great need for inference, questioning, and guessing by the listener.</items>
                        </term_definitions>
                        <term_definitions code="at0083">
                            <items id="text">Mute, global aphasia</items>
                            <items id="description">No usable speech or auditory comprehension.</items>
                        </term_definitions>
                        <term_definitions code="at0085">
                            <items id="text">Extinction and inattention</items>
                            <items id="description">Observation of any evidence of extinction or inattention.</items>
                            <items id="comment">Also known as 'neglect'.</items>
                        </term_definitions>
                        <term_definitions code="at0089">
                            <items id="text">No abnormality</items>
                            <items id="description">No abnormality observed.</items>
                        </term_definitions>
                        <term_definitions code="at0090">
                            <items id="text">Inattention, or bilateral extinction in one sensory modality</items>
                            <items id="description">Visual, tactile, auditory, spatial, or personal inattention, or extinction to bilateral simultaneous stimulation in one of the sensory modalities.</items>
                        </term_definitions>
                        <term_definitions code="at0091">
                            <items id="text">Profound hemi-inattention or extinction to more than one modality</items>
                            <items id="description">Profound hemi-inattention or extinction to more than one modality; does not recognize own hand or orients to only one side of space.</items>
                        </term_definitions>
                        <term_definitions code="at0097">
                            <items id="text">NIHSS saat keluar RS</items>
                            <items id="description">The sum of the ordinal scores recorded for each of the fifteen component responses.</items>
                        </term_definitions>
                        <term_definitions code="at0098">
                            <items id="text">*Tree(pt)</items>
                            <items id="description">*@ internal @(pt)</items>
                        </term_definitions>
                        <term_definitions code="at0120">
                            <items id="text">Motor - left arm</items>
                            <items id="description">Observation of left arm motor function.</items>
                            <items id="comment">Implementation note: while the UNTESTABLE value has been included in this ordinal to allow for inclusion in user interface picklists, the NIHSS applies no numerical score from this selection to the 'Total score'.</items>
                        </term_definitions>
                        <term_definitions code="at0121">
                            <items id="text">No drift</items>
                            <items id="description">Limb holds 90 (or 45) degrees for full 10 seconds.</items>
                        </term_definitions>
                        <term_definitions code="at0122">
                            <items id="text">Drift</items>
                            <items id="description">Limb holds 90 (or 45) degrees, but drifts down before full 10 seconds; does not hit bed or other support.</items>
                        </term_definitions>
