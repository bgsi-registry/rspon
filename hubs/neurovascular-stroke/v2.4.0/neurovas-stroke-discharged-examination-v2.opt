                            <items id="text">Bowels</items>
                            <items id="description">Over the previous week, occasional accident is once per week.</items>
                        </term_definitions>
                        <term_definitions code="at0009">
                            <items id="text">Bathing</items>
                            <items id="description">Independent = without supervision or help when getting in and out of the bath and when washing.</items>
                        </term_definitions>
                        <term_definitions code="at0010">
                            <items id="text">Dependent</items>
                            <items id="description">Needs assistance with baths or showers.</items>
                        </term_definitions>
                        <term_definitions code="at0011">
                            <items id="text">Independent</items>
                            <items id="description">No help required when getting in and out of the bath or when washing.</items>
                        </term_definitions>
                        <term_definitions code="at0012">
                            <items id="text">Dressing</items>
                            <items id="description">Ability to choose clothes, put them on and fasten them.</items>
                        </term_definitions>
                        <term_definitions code="at0013">
                            <items id="text">Dependent</items>
                            <items id="description">Requires assistance choosing and putting on clothes.</items>
                        </term_definitions>
                        <term_definitions code="at0014">
                            <items id="text">Needs help but can do about half unaided</items>
                            <items id="description">Needs help only with buttons, zippers but can put on clothes unaided.</items>
                        </term_definitions>
                        <term_definitions code="at0016">
                            <items id="text">Indeks Barthel saat keluar RS</items>
                            <items id="description">Sum of the individual scores assigned for each of the contributing variables.</items>
                            <items id="comment">A score of 0-9 indicates high dependency, 10-19 moderate dependency and 20 is independent.</items>
                        </term_definitions>
                        <term_definitions code="at0017">
                            <items id="text">Mobility</items>
                            <items id="description">Ability to get about the house or institution.</items>
                        </term_definitions>
                        <term_definitions code="at0018">
